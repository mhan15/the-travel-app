        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var start_gdt = new GlideDateTime(current.getDisplayValue('from'));

    var dur = new TravelUtil().daysBeforeStart(start_gdt);

    current.short_description = dur;

    if (dur <= 10) {
        current.urgency = 1;
    } else if (dur <= 30) {
        current.urgency = 2;
    } else {
        current.urgency = 3;
